{
    const selectElement = document.getElementById("place");
    const customPlaceInput = document.getElementById("customplace");

    // カスタムプレイスの値を取得
    const customPlaceValue = customPlaceInput.value;

    // ローカルストレージに保存
    localStorage.setItem("customPlace", customPlaceValue);

    // 新しい選択肢をセレクトボックスに追加
    const addElement = document.getElementById("place");
    const newPlace = new Place(customPlaceValue, customPlaceValue);
    addElement.appendChild(newPlace);

    // カスタムプレイスを選択状態にする
    newOption.selected = true;

    // カスタムプレイスの入力をクリア
    customPlaceInput.value = "";

  }

